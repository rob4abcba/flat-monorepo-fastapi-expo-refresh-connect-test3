curl -X POST https://api.elevenlabs.io/v1/text-to-speech/$ELEVEN_VOICE_ID \
  -H "xi-api-key: $ELEVEN_API_KEY" \
  -H "Content-Type: application/json" \
  -d '{
    "text": "你好，欢迎使用我们的语音应用。",
    "model_id": "eleven_multilingual_v2",
    "voice_settings": {
      "stability": 0.3,
      "similarity_boost": 0.7
    }
  }' --output output.mp3
